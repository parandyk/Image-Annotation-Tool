             xmlns:dialogHostAvalonia="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
             xmlns:app="clr-namespace:ImageAnnotationTool"
             xmlns:semi="https://irihi.tech/semi"
             xmlns:u-semi="https://irihi.tech/ursa/themes/semi"
             x:Class="ImageAnnotationTool.App"
             RequestedThemeVariant="Dark"
             Name="Image Annotation Tool">
    <Application.DataTemplates>
        <app:ViewLocator/>
    </Application.DataTemplates>
    
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
    
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="Styles/AppDefaultStyles.axaml"></StyleInclude>
        <styling:FluentAvaloniaTheme TextVerticalAlignmentOverrideBehavior="AlwaysEnabled"/>
        <semi:SemiTheme Locale="en-us"/>
        <u-semi:SemiTheme Locale="en-us"/>
        <dialogHostAvalonia:DialogHostStyles/>
    </Application.Styles>
    
    <Application.Resources>
        <SolidColorBrush x:Key="SystemControlHighlightAccentBrush" Color="{DynamicResource SystemAccentColor}" />
